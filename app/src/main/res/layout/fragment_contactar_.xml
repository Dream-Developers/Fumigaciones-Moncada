<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Contactar_Fragment">

    <!-- TODO: Update blank fragment layout -->


    <Button
        android:id="@+id/btn_facebook"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/facebook"
        app:layout_constraintBottom_toBottomOf="@+id/btn_telefono"
        app:layout_constraintEnd_toStartOf="@+id/btn_telefono"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_telefono" />

    <Button
        android:id="@+id/btn_telefono"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/telefono"
        app:layout_constraintBottom_toBottomOf="@+id/btn_instagram"
        app:layout_constraintEnd_toStartOf="@+id/btn_instagram"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_facebook"
        app:layout_constraintTop_toTopOf="@+id/btn_instagram"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_instagram"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/instagram"
        app:layout_constraintEnd_toStartOf="@+id/btn_web"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_telefono"
        app:layout_constraintTop_toTopOf="@+id/btn_web" />

    <Button
        android:id="@+id/btn_web"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="564dp"
        android:background="@drawable/web"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_instagram"
        app:layout_constraintTop_toTopOf="@+id/recycler_contactar"
        tools:ignore="MissingConstraints" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_contactar"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="0dp" />


</androidx.constraintlayout.widget.ConstraintLayout>